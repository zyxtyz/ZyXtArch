{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0d151f90",
                "editor.background": "#0d151f90",
                "editor.foreground": "#c2c4c7",
                "text": "#c2c4c7",
                "text.muted": "#c2c4c770",
                "text.ignored": "#c2c4c740",
                "text.placeholder": "#c2c4c750",
                "ignored": "#c2c4c730",
                "element.hover": "#c2c4c730",
                "ghost_element.hover": "#44529330",
                "ghost_element.selected": "#44529330",
                "ghost_element.active": "#44529360",
                "border": "#0d151f",
                "editor.highlighted_line.background": "#44529310",
                "editor.active_line.background": "#44529310",
                "panel.background": "#0d151f90",
                "title_bar.background": "#0d151f90",
                "title_bar.inactive_background": "#0d151f90",
                "status_bar.background": "#0d151f90",
                "drop_target.background": "#0d151f90",
                "elevated_surface.background": "#0d151f",
                "toolbar.background": "#0d151f90",
                "tab_bar.background": "#0d151f90",
                "tab.inactive_background": "#0d151f90",
                "tab.active_background": "#44529330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0d151f",
                "scrollbar.thumb.background": "#445293",
                "editor.gutter.background": "#0d151f90",
                "terminal.background": "#0d151f10",
                "terminal.foreground": "#c2c4c7",
                "terminal.dim_foreground": "#c2c4c7",
                "terminal.bright_foreground": "#c2c4c7",
                "terminal.ansi.black": "#0d151f",
                "terminal.ansi.red": "#445293",
                "terminal.ansi.green": "#4B5A9C",
                "terminal.ansi.yellow": "#54788F",
                "terminal.ansi.blue": "#5D839B",
                "terminal.ansi.magenta": "#61869D",
                "terminal.ansi.cyan": "#678CA2",
                "terminal.ansi.white": "#c2c4c7",
                "terminal.ansi.bright_black": "#5c6571",
                "terminal.ansi.bright_red": "#445293",
                "terminal.ansi.bright_green": "#4B5A9C",
                "terminal.ansi.bright_yellow": "#54788F",
                "terminal.ansi.bright_blue": "#5D839B",
                "terminal.ansi.bright_magenta": "#61869D",
                "terminal.ansi.bright_cyan": "#678CA2",
                "terminal.ansi.bright_white": "#c2c4c7",
                "modified": "#445293",
                "syntax": {
                    "attribute": {
                        "color": "#445293"
                    },
                    "boolean": {
                      "color": "#4B5A9C"
                    },
                    "comment": {
                      "color": "#5c6571"
                    },
                    "comment.doc": {
                      "color": "#5c6571"
                    },
                    "constant": {
                      "color": "#4B5A9C"
                    },
                    "function": {
                      "color": "#678CA2"
                    },
                    "keyword": {
                      "color": "#5D839B"
                    },
                    "number": {
                      "color": "#61869D"
                    },
                    "operator": {
                      "color": "#5D839B"
                    },
                    "string": {
                      "color": "#445293"
                    },
                    "variable": {
                      "color": "#4B5A9C"
                    }
                },
                "players": [
                  {
                    "cursor": "#445293",
                    "background": "#0d151f",
                    "selection": "#44529330"
                  }
                ]
            }
        }
    ]
}
